<script>
    let errorEvents = [];
    window.addEventListener('error', function (event) {
        errorEvents.push(`error: ${JSON.stringify(event)}`);
    });
    window.addEventListener('unhandledrejection', function (event) {
        errorEvents.push(`rejectionhandled: ${JSON.stringify(event)}, reason: ${event.reason}, data: ${event.data}`);
    });
    window.addEventListener('rejectionhandled', function (event) {
        errorEvents.push(`rejectionhandled: ${JSON.stringify(event)}, reason: ${event.reason}, data: ${event.data}`);
    });
    let tabVisibilityLog = ['Initial state: ' + document.visibilityState];

    function checkTabFocused() {
        if (document.visibilityState === 'visible') {
            tabVisibilityLog.push('State change event: browser tab has focus');
        } else {
            tabVisibilityLog.push('State change event: browser tab does NOT have focus');
        }
    }

    document.addEventListener('visibilitychange', checkTabFocused);

    document.addEventListener("DOMContentLoaded", function () {
        const challengeForm = document.getElementById("challenge_form");
        const cancelForm = document.getElementById("cancel_form");
        const submitButton = document.getElementById("submitButton");
        const cancelButton = document.getElementById("cancelButton");

        let isSubmitting = false;
        let intervalId;

        function disableButtons() {
            if (submitButton) {
                submitButton.disabled = true;
                submitButton.style.cursor = "not-allowed";
            }
            if (cancelButton) {
                cancelButton.disabled = true;
                cancelButton.style.cursor = "not-allowed";
            }
        }

        function handleSubmit(event) {
            if (isSubmitting) {
                event.preventDefault();
                return;
            }
            isSubmitting = true;
            disableButtons();
            if (intervalId) clearInterval(intervalId);
            // Let the form submit normally
        }

        if (challengeForm) challengeForm.addEventListener("submit", handleSubmit);
        if (cancelForm) cancelForm.addEventListener("submit", handleSubmit);

        intervalId = setInterval(async () => {
            if (isSubmitting) return; // already submitting, do nothing

            try {
                const res = await fetch('https://acs-challenge.apata.io/v1/PollTransaction', {
                    method: 'POST',
                    body: JSON.stringify({
                        pollToken: "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..XNbh1oQyj_SU0wf-UHkC08AX_VoJZYHDQmDKWRuhEXo"
                    }),
                    headers: {
                        'Content-type': 'application/json; charset=UTF-8',
                    }
                });

                if (res.ok) {
                    const data = await res.json();
                    if (data.state === "COMPLETED" && !isSubmitting) {
                        isSubmitting = true;
                        disableButtons();
                        clearInterval(intervalId);
                        if (challengeForm) {
                            challengeForm.submit();
                        }
                    }
                }
            } catch (e) {
                await fetch('https://acs.apata.io/v1/LogError', {
                    method: 'POST',
                    body: JSON.stringify({
                        errorJson: {
                            errorMessage: e.message,
                            errorStack: e.stack,
                            pollToken: "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..XNbh1oQyj_SU0wf-UHkC08AX_VoJZYHDQmDKWRuhEXo",
                            financialInstitution: "Wise",
                            transactionId: "803d5ed2-bc23-4bee-b198-de11cfa7a069",
                            tabVisibilityLog: tabVisibilityLog,
                            errorEvents: errorEvents
                        }
                    }),
                    headers: {
                        'Content-type': 'application/json; charset=UTF-8',
                    }
                });
            }
        }, 3000);
    });
</script>

